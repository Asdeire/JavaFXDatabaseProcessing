<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>
<VBox spacing="10" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/17"
      fx:controller="com.asdeire.MainController">
    <TableView fx:id="tableView">
        <columns>
            <TableColumn fx:id="idColumn" text="ID"/>
            <TableColumn fx:id="firstNameColumn" text="First Name"/>
            <TableColumn fx:id="lastNameColumn" text="Last Name"/>
            <TableColumn fx:id="emailColumn" text="Email"/>
            <TableColumn fx:id="phoneColumn" text="Phone"/>
            <TableColumn fx:id="addressColumn" text="Address"/>
            <TableColumn fx:id="cityColumn" text="City"/>
            <TableColumn fx:id="countryColumn" text="Country"/>
        </columns>
    </TableView>
</VBox>
